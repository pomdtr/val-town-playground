{
  "name": "vt-playground",
  "type": "module",
  "version": "0.0.10",
  "author": "Achille Lacoin",
  "description": "A playground component for Val Town",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist/*",
