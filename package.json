    "dist",
    "lib"
  ],
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "codemirror": "^6.0.1",
    "lit": "^3.1.0",
    "stringify-object": "^5.0.0"
  },
  "devDependencies": {
    "@types/stringify-object": "^4.0.5",
    "chokidar-cli": "^3.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
